{
  "name": "rfssc-website",
  "version": "1.0.0",
  "type": "module",
  "description": "Website for the Fellowship of Sub-Saharan Culture",
  "main": "src/main.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint src --ext ts,js,json",
    "lint:fix": "eslint src --ext ts,js,json --fix",
    "format": "prettier --write src/**/*.{ts,js,css,html,json}",
    "type-check": "tsc --noEmit",
    "analyze": "vite-bundle-analyzer"
  },
  "keywords": [
    "rotary",
    "fellowship",
    "sub-saharan",
    "culture",
    "africa",
    "international"
  ],
  "author": "Fellowship of Sub-Saharan Culture",
  "license": "MIT",
  "devDependencies": {
    "vite": "^5.0.0",
    "typescript": "^5.3.0",
    "tailwindcss": "^3.4.0",
    "@vitejs/plugin-legacy": "^5.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "prettier": "^3.1.0",
    "vitest": "^1.0.0",
    "playwright": "^1.40.0",
    "vite-bundle-analyzer": "^0.7.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0"
  },
  "dependencies": {
    "modern-normalize": "^2.0.0"
  }
}
